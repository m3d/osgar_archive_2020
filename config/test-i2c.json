{
  "version": 2,
  "robot": {
    "modules": {
      "app": {
          "driver": "application",
          "in": ["raw"],
          "out": ["raw"],
          "init": {}
      },
      "boat": {
          "driver": "marina",
          "in": ["i2c"],
          "out": ["cmd"],
          "init": {}
      },
      "i2c": {
          "driver": "i2c",
          "in": ["cmd"],
          "out": ["i2c"],
          "init": {
            "port": 1
          }
      }
    },
    "links": [["i2c.i2c", "boat.i2c"], ["boat.cmd", "i2c.cmd"]]
  }
}
