{
  "version": 2,
  "robot": {
    "modules": {
      "app": {
          "driver": "application",
          "in": ["raw"],
          "out": ["raw"],
          "init": {}
      },
      "marina": {
          "driver": "i2c",
          "in": ["cmd"],
          "out": ["i2c"],
          "init": {
            "port": 1
          }
      }
    },
    "links": [["marina.i2c", "app.raw"], ["app.raw", "marina.cmd"]]
  }
}
