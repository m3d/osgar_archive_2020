{
  "version": 2,
  "robot": {
    "modules": {
      "app": {
          "driver": "application",
          "in": ["encoders"],
          "out": ["desired_speed"],
          "init": {}
      },
      "eduro": {
          "driver": "eduro",
          "in": ["can", "desired_speed"],
          "out": ["can", "encoders"],
          "init": {}
      },
      "can": {
          "driver": "can",
          "in": ["raw", "can"],
          "out": ["can", "raw"],
          "init": {"speed": 250000, "canopen":true}
      },
      "serial": {
          "driver": "serial",
          "in": ["raw"],
          "out": ["raw"],
          "init": {"port": "/dev/ttyS0", "speed": 115200,
                   "rtscts":true, "reset":true}
      }
    },
    "links": [["serial.raw", "can.raw"], 
              ["can.raw", "serial.raw"],
              ["eduro.can", "can.can"],
              ["can.can", "eduro.can"],
              ["eduro.encoders", "app.encoders"],
              ["app.desired_speed", "jd.desired_speed"]]
  }
}
