{
  "version": 2,
  "robot": {
    "modules": {
      "app": {
          "driver": "application",
          "in": ["can"],
          "out": ["can"],
          "init": {}
      },
      "can": {
          "driver": "can",
          "in": ["raw", "can"],
          "out": ["can", "raw"],
          "init": {"speed": "500k", "canopen":true}
      },
      "serial": {
          "driver": "serial",
          "in": ["raw"],
          "out": ["raw"],
          "init": {"port": "/dev/ttyS0", "speed": 115200,
                   "rtscts":true, "reset":false}
      }
    },
    "links": [["serial.raw", "can.raw"],
              ["can.raw", "serial.raw"],
              ["app.can", "can.can"],
              ["can.can", "app.can"]
      ]
  }
}
